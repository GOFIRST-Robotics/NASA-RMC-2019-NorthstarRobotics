<!-- This is the exmaple from the tutorial-->

<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find bigrdigr_gazebo)/worlds/test.world"/>
    <!-- more default parameters can be changed here -->
  </include>

  <param name="bigrdigr_description" command="$(find xacro)/xacro --inorder $(find bigrdigr_description)/urdf/bigrdigr.urdf.xacro" />

  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param bigrdigr_description -urdf -model bigrdigr -x 0 -y 0 -z 0" />


</launch>
